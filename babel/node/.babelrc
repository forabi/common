{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/transform-runtime"
  ],
  "sourceMap": "both",
  "retainLines": true
}
